*$FROM Library
$EXTERN Add;

$ENTRY Go {
  CompoundSymbol = "escapes: \x42\n\t\r\'\"\<\>\(\)";
  Chars = 'escapes: \x42\n\t\r\\\'\"\<\>\(\)';
  BadEscape = '\q';
  unclosed_quote = 'abc;
  unclosed_compound = "abc;
  bad_escaped_character = \q;

  SpecialSymbols
    , \x42 \n \r \t \' \" \< \> \( \) \\
    : '\x42\n\r\t\'\"\<\>\(\)'
    = <Go 1>;

  e.Other = e.Other;
}
